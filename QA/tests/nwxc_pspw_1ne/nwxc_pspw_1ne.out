# $Id: nwxc_pspw_1he.nw 23460 2013-01-17 00:11:15Z d3y133 $
echo

#memory 950 mb

start nwxc_dat

charge 0

geometry
  Ne 0.0 0.0 0.0
end

set nwpw:minimizer 2

# LDA exchange-correlation

nwpw
   simulation_cell
     boundary_conditions aperiodic
     ngrid 32 32 32
     SC 20.0      #Not checking automatic unit cell generation
   end
   lcao #added 3/26/10 because of changed default
   xc lda
end
task pspw energy

# GGA exchange-correlation

nwpw
   simulation_cell
     boundary_conditions aperiodic
     ngrid 32 32 32
     SC 20.0      #Not checking automatic unit cell generation
   end
   lcao #added 3/26/10 because of changed default
   xc pbe96
end
task pspw energy

