# He  -2.8937015127         -0.0399725506

echo

start He_b2plyp_cc-vqz

memory total 1000 mb

geometry
 He     0.0 0.0 0.0
end

basis "ao basis" spherical
  * library cc-pvqz
end

mp2
  freeze 0
  scratchdisk 4096
end

dft
  xc HFexch 0.53 becke88 0.47 lyp 0.73 mp2 0.27
  rungfive 1 # semidirect
  convergence energy 1e-8
  iterations 100
end

