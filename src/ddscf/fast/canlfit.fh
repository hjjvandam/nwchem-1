c
c     Common block for rapid interpolation of the radial
c     component of the potential due to a general Gaussian source
c
c     See anlfit.F
c
      integer maxn,npt,order, maxnl
      double precision xhi
      parameter (maxn=10, npt=1500, xhi=7.0d0, order=5)
      parameter (maxnl = ((maxn+2)*(maxn+3)/4))
      double precision scale, table, prefac
      integer mapnl
      common /canlfit/scale,table(0:order+1,0:npt-1,maxnl),
     $     prefac(0:maxn,0:maxn), mapnl(0:maxn,0:maxn)
c
