 argument  1 = esp-nobq.nw



============================== echo of input deck ==============================
memory 2000 mb
start h2o

echo

permanent_dir  ./perm

scratch_dir   ./data

charge 0
basis noprint
* library 6-31G
end


scf
thresh 1.0e-10
tol2e 1.0e-10
singlet
rhf
end


dft
print low
xc b3lyp
iterations 200
end

esp
print medium
end



md
 system h2o_ref
 cutoff 1.5 qmmm 1.5
 noshake solute
end



qmmm
mm_charges exclude all
end

task dft qmmm energy
task qmmm esp
