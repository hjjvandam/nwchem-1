permanent_dir ./data
scratch_dir ./perm

memory 800 mb
echo

title "  h2+h    "

start h2+h 

geometry noautoz
symmetry c1 
Fe  0.93453582  0.0 0.0
H   0.0 0.0 0.0
Fe  -0.93453582  0.0 0.0
end

scf
  doublet
  uhf
  ADAPT off
  SYM off
end

basis spherical
 * Library 6-311G* 
end
driver
 xyz
end
mymd
 temp 298.15
 nsteps 10
 timestep 0.5
end
mymc
 temp 298.15
 mcsteps 4 
 stepsize 0.5
 ratio 0.5
end

