permanent_dir ./data
scratch_dir ./perm

memory 800 mb
echo

title "       hexaachloroethene C2Cl6"

start c2cl6

geometry
h  0.93452655 0.0 0.0 
h  0.0  0.0   0.0
h  -0.93452655 0.0 0.0
end

scf
  doublet
  rohf
  ADAPT off
  SYM off
end

basis spherical
 * Library 6-31+G* 
end

mymd
 temp 298.15
 nsteps 20
 timestep 0.5
end

